
repeat wait() until game:IsLoaded() and game.Players.LocalPlayer:FindFirstChild("PlayerGui")

-- Màu mặc định nếu rainbow chưa chạy
_G.Color = Color3.fromRGB(0, 180, 255)

-- Tạo GUI hiển thị loại script, FPS và tên tác giả
local ScreenGui = Instance.new("ScreenGui", game:GetService("CoreGui"))
ScreenGui.Name = "InfoFPS_GUI"

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 220, 0, 100)
Frame.Position = UDim2.new(0, 10, 0, 10)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BackgroundTransparency = 0.1
Frame.Active = true
Frame.Draggable = true
Instance.new("UICorner", Frame)

-- Loại script
local TypeLabel = Instance.new("TextLabel", Frame)
TypeLabel.Size = UDim2.new(1, 0, 0.3, 0)
TypeLabel.Position = UDim2.new(0, 0, 0, 0)
TypeLabel.Text = "[Fix Lag PvP"
TypeLabel.TextColor3 = _G.Color
TypeLabel.Font = Enum.Font.GothamBold
TypeLabel.TextScaled = true
TypeLabel.BackgroundTransparency = 1

-- FPS Label
local FPSLabel = Instance.new("TextLabel", Frame)
FPSLabel.Size = UDim2.new(1, 0, 0.35, 0)
FPSLabel.Position = UDim2.new(0, 0, 0.3, 0)
FPSLabel.Text = "FPS: 0"
FPSLabel.TextColor3 = _G.Color
FPSLabel.Font = Enum.Font.GothamBold
FPSLabel.TextScaled = true
FPSLabel.BackgroundTransparency = 1

-- Made by
local AuthorLabel = Instance.new("TextLabel", Frame)
AuthorLabel.Size = UDim2.new(1, 0, 0.35, 0)
AuthorLabel.Position = UDim2.new(0, 0, 0.65, 0)
AuthorLabel.Text = "Made by Qminh (kenny)"
AuthorLabel.TextColor3 = _G.Color
AuthorLabel.Font = Enum.Font.Gotham
AuthorLabel.TextScaled = true
AuthorLabel.BackgroundTransparency = 1

-- FPS tính toán
local RunService = game:GetService("RunService")
local frameCount = 0
local lastUpdate = tick()

RunService.RenderStepped:Connect(function()
    frameCount = 1
    local now = tick()
    if now - lastUpdate >= 1 then
        local fps = math.floor(frameCount / (now - lastUpdate))
        FPSLabel.Text = "[FPS: " .. tostring(fps) .. "]"
        frameCount = 0
        lastUpdate = now
    end
end)

-- Rainbow hiệu ứng
task.spawn(function()
    local hue = 0
    while true do
        hue = (hue + 0.01) % 1
        local color = Color3.fromHSV(hue, 1, 1)
        pcall(function()
            TypeLabel.TextColor3 = color
            FPSLabel.TextColor3 = color
            AuthorLabel.TextColor3 = color
        end)
        RunService.RenderStepped:Wait()
    end
end)
-- Safe Fix Lag - Compatible with all executors
pcall(function()
    local Lighting = game:GetService("Lighting")
    local Terrain = workspace:FindFirstChildOfClass("Terrain")

    Lighting.GlobalShadows = false
    Lighting.FogEnd = 1e7
    Lighting.Brightness = 1
    Lighting.ClockTime = 14
    Lighting.OutdoorAmbient = Color3.fromRGB(127,127,127)
    Lighting.Ambient = Color3.fromRGB(127,127,127)

    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("ParticleEmitter") or obj:IsA("Trail") or obj:IsA("Explosion") or obj:IsA("Smoke") or obj:IsA("Fire") or obj:IsA("Sparkles") then
            pcall(function() obj:Destroy() end)
        elseif obj:IsA("Decal") or obj:IsA("Texture") then
            pcall(function() obj.Transparency = 0.8 end)
        elseif obj:IsA("MeshPart") or obj:IsA("Part") then
            pcall(function()
                obj.Material = Enum.Material.SmoothPlastic
                obj.Reflectance = 0
            end)
        end
    end

    if Terrain then
        Terrain.WaterWaveSize = 0
        Terrain.WaterWaveSpeed = 0
        Terrain.WaterReflectance = 0
        Terrain.WaterTransparency = 0.9
    end

    settings().Rendering.QualityLevel = Enum.QualityLevel.Level02
end)

